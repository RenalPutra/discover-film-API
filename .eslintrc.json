{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "never",
              "jsx": "never",
              "ts": "never",
              "tsx": "never"
            }
         ],
         "object-curly-newline": ["error", "always"],
         "linebreak-style": 0,
         "no-console": "off",
         "no-underscore-dangle": ["error", "allow"]
         
    }
}
